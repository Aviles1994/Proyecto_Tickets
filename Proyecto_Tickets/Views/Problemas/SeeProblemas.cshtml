@model  List<Proyecto_Tickets.Models.TableViewsModels.SeeProblema>

@{
    ViewBag.Title = "SeeProblemas";
    Layout = "~/Views/Shared/_LayoutDiseño.cshtml";

}


<main>
    <div class="div_formulario">
        <table class="table_busqueda">
            <tr>
                <th>Nombre del Problema</th>
                <th>Descripción Problema</th>
                <th>Descripción Solución</th>
                <th>Nombre Pantalla</th>
                
            </tr>

            @foreach (var oElemento in Model)
            {
                <tr>
                    <td>@oElemento.Nombre_Problema</td>
                    <td>@oElemento.descripción_problema</td>
                    <td>@oElemento.descrpcion_solucion</td>
                    <td>@oElemento.nombre_pantalla</td>
                </tr>
            }
        </table>
    </div>

</main>

@section scripts
{
    <script>
        $(document).ready(function () {
            $('.sidebar-menu').tree();

        })
    </script>
    }